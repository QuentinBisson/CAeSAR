{% extends "CaesarUserBundle::layout.html.twig" %}

{% block bodyTitle %}
<h1>CAeSAR</h1>
{% endblock %}

{% block body %}
<section id="main-form">
	<form action="#" method="post" autocomplete="off">
		<input type="text" name="main_form_text" placeholder="Entrez un code BU, ISBN, identifiant ...">
		<div class="main-form-actions">
			<input type="submit" name="emprunter" value="Emprunter">
			<input type="submit" name="rendre" value="Rendre" disabled="disabled">
			<input type="submit" name="identifier" value="Identifier" disabled="disabled">
		</div>
	</form>
</section>
{% endblock %}