{% extends "CaesarUserBundle::layout.html.twig" %}

{% block bodyTitle %}
    <h1>{{ 'user.profile.modify.title'|trans({}, 'CaesarUserBundle') }}</h1>
{% endblock %}

{% block caesar_user_content %}
    <form class="form-client" action="{{ path('caesar_client_modify_user') }}" method="post">
        {{ form_widget(form) }}
        <input type="submit" value="{{ 'user.profile.label.modify'|trans({}, 'CaesarUserBundle') }}">
    </form>
{% endblock caesar_user_content %}
