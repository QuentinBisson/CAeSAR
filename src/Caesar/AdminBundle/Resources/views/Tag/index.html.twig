{% extends "CaesarAdminBundle::layout.html.twig" %}

{% block stylesheets %}
    {{parent()}}
<link rel="stylesheet" type="text/css" href="{{ asset('css/table.css') }}" />
<link rel="stylesheet" type="text/css" href="{{ asset('bundles/caesaradmin/css/admin.css') }}" />
{% endblock %}

{% block breadCrumb%}
    {{parent()}}
<li data-header="5" class="active"><span>{{'admin.breadcrumb.tags.list'|trans({})}}</span></li>
{% endblock %}

{% block contentTitle %}{{'admin.title.tags.list'|trans({})}}{% endblock %}

{% block contentBody %}
    <section class="contentBody">
     {% include 'CaesarAdminBundle:Tag:list.html.twig' with {'tags' : tags, 'page' : page, 'direction' : direction, 'pagination' : pagination, 'count': count}  %}
    </section>
{% endblock %}
{% block javascripts %}
{{parent()}}
        <script type="text/javascript" src="{{ asset('js/sort.js') }}" ></script>
{% endblock %}